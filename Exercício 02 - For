import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		int valor = 0, i = 0, X = 0, Y = 0;
		
		for (i = 1; i <= num; i++) {
			valor = sc.nextInt();
			if (valor >= 10 && valor <=20) {
				X = X + 1;
			}
			else {
				Y = Y + 1;
			}
			
		}
	
		System.out.printf("%d in%n", X);
		System.out.printf("%d out%n", Y);
		
		sc.close();
	}

}
