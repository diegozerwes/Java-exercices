import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		
		Scanner sc = new Scanner(System.in);
		
		
		int a, b, c, d, P;
		
		a = sc.nextInt();
		sc.nextLine();
		b = sc.nextInt();
		sc.nextLine();
		c = sc.nextInt();
		sc.nextLine();
		d = sc.nextInt();
		sc.nextLine();
		
		P = (a*b-c*d);
	
		
		System.out.println("Produto =" + P);
		
	
		sc.close();
	
	}

}
