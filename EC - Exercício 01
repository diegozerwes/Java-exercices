import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int numero;
		
		numero = sc.nextInt();
		
		if (numero >= 0) {
			System.out.printf(" O numero  %d não é negativo", numero);
		}
		
		else {
			System.out.printf("O numero %d é negativo", numero);
		}
			
		
		sc.close();
	}

}
