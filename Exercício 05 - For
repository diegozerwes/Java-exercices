import java.util.Locale;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		Locale.setDefault(Locale.US);
		
		int num = sc.nextInt();
		int conta = 1;
		
		for (int i = 1; i <= num; i++) {
			conta = conta * i;
				
			}
	
		System.out.println(conta);
		
		
		sc.close();
	}

}
